  <!-- 查询待确认的还款记录 -->
  <select id="selectPendingConfirmations" resultMap="BaseResultMap">
    SELECT op.*
    FROM outcome_participant op
    INNER JOIN outcome o ON op.outcome_id = o.id
    WHERE o.repay_flag = 2
      AND o.deleted_flag = 0
      AND op.user_id = #{creditorId}
      AND (op.confirm_status IS NULL OR op.confirm_status = 0)
    ORDER BY o.pay_datetime DESC
  </select>

