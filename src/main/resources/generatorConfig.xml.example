<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <!-- MySQL 8 驱动 -->
    <classPathEntry location="C:/Users/ckun0/.m2/repository/com/mysql/mysql-connector-j/9.5.0/mysql-connector-j-9.5.0.jar"/>

    <context id="MySQL8Tables" targetRuntime="MyBatis3">
        <property name="javaFileEncoding" value="UTF-8"/>

        <!-- 1. JDBC 配置 -->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://xxx.xxx.xxx.xxx:3306/databasename?useSSL=false&amp;serverTimezone=Asia/Tokyo"
                        userId=""
                        password="">
        <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>
        
		<javaTypeResolver type="org.mybatis.generator.internal.types.JavaTypeResolverDefaultImpl">
		    <property name="useJSR310Types" value="true"/>
		</javaTypeResolver>        

        <!-- 2. 实体类生成位置 -->
        <javaModelGenerator targetPackage="com.zhangben.backend.model"
                            targetProject="src/main/java"/>

        <!-- 3. XML 映射文件位置 -->
        <sqlMapGenerator targetPackage="mapper"
                         targetProject="src/main/resources"/>

        <!-- 4. Mapper 接口位置 -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.zhangben.backend.mapper"
                             targetProject="src/main/java"/>

        <!-- 5. 指定要生成的表 -->
        <table tableName="outcome" domainObjectName="Outcome"/>


    </context>
</generatorConfiguration>